class Q41
{
	public static void main(String[] args) 
	{
		for (int i=1;i<=5 ;i++ )
		{
			for (int j=1;j<=(5-i) ;j++ )
			{
				System.out.print(" ");
			}
     		for (char k='A';k<=((2*i)-65) ;k++ )   
			{
				System.out.print(k);
			}
          System.out.println();
		}
	}
}
