class T
{
	public static void main(String[] args) 
	{ 
		for(char i='A';i<='E';i++)
		{
			for(int j=i-1;j>='A';j--)
			{
               System.out.print(" ");
			}
			for (char k='E';k>=i ;k-- )
			{
				 System.out.print("*");
			}
			System.out.println();
		}
	}
}
