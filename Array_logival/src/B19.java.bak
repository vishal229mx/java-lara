//read the index which has equal value in the left and right sides
import java.util.Arrays;
class B19
{
	public static void main(String[] args) 
	{
		int sum = 0;
		int[] x = {1,2,1,5,6,7,4,8,9,8};
			System.out.println("array content:" + Arrays.toString(x));
			for (int i = 1 ; i < x.length - 1 ; i++ )
			{
				if (x[i-1] == x[i+1])
				{
					System.out.print(i + ", ");
				}
			}
	}
}